{
  "name": "qwik-backend",
  "version": "1.0.0",
  "description": "Backend API for Qwik e-commerce app",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc --noEmitOnError false",
    "start": "node dist/index.js",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && prisma db push --accept-data-loss && tsc --noEmitOnError false",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "set DATABASE_URL=postgresql://postgres:sHIVASHIVA2@db.xhhostcixgxhsvjfxaly.supabase.co:5432/postgres && npx prisma db push",
    "prisma:seed": "set DATABASE_URL=postgresql://postgres:sHIVASHIVA2@db.xhhostcixgxhsvjfxaly.supabase.co:5432/postgres && tsx prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "api",
    "express",
    "prisma",
    "ecommerce"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
