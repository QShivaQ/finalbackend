    "@envelop/depth-limit": "^7.0.0",
    "@envelop/filter-operation-type": "^9.0.0",
    "@envelop/rate-limiter": "^9.0.0",
    "@pothos/core": "^4.10.0",
    "@pothos/plugin-prisma": "^4.12.0",
    "@pothos/plugin-scope-auth": "^4.1.6",
    "@pothos/plugin-validation": "^4.2.0",
    "@prisma/client": "^6.19.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dataloader": "^2.2.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "graphql": "^16.12.0",
    "graphql-redis-subscriptions": "^2.7.0",
    "graphql-scalars": "^1.25.0",
    "graphql-yoga": "^5.16.2",
    "helmet": "^8.0.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.1"
