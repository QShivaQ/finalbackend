  @@index([created_at])
  @@index([latest])
  @@index([parent_id], map: "_products_v_parent_idx")
  @@index([updated_at])
  @@index([version_meta_image_id], map: "_products_v_version_meta_version_meta_image_idx")
  @@index([version__status], map: "_products_v_version_version__status_idx")
  @@index([version_created_at], map: "_products_v_version_version_created_at_idx")
  @@index([version_deleted_at], map: "_products_v_version_version_deleted_at_idx")
  @@index([version_slug], map: "_products_v_version_version_slug_idx")
  @@index([version_updated_at], map: "_products_v_version_version_updated_at_idx")
  @@map("_products_v")
}

