  uuid       String?                            @map("_uuid") @db.VarChar
  block_name String?                            @db.VarChar
  pages_v    pages_v                            @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "_pages_v_blocks_banner_parent_id_fk")

