  version_title                  String?                          @db.VarChar
  version_description            Json?
  version_inventory              Decimal?                         @default(0) @db.Decimal
  version_enable_variants        Boolean?
  version_price_in_u_s_d_enabled Boolean?
  version_price_in_u_s_d         Decimal?                         @db.Decimal
  version_meta_title             String?                          @db.VarChar
